{
  "name": "prismic-reactjs",
  "version": "1.3.2",
  "description": "render prismic rich text as React Element",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "webpack --debug; webpack -p; babel src -d dist/cjs; BABEL_ENV=esm babel src -d dist/esm",
    "analyze": "webpack -p --analyze",
    "dev": "webpack -d --watch",
    "prepublish": "npm run build",
    "test": "mocha --require @babel/register test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismicio/prismic-reactjs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prismicio/prismic-reactjs/issues"
  },
  "homepage": "https://github.com/prismicio/prismic-reactjs#readme",
  "dependencies": {
    "prismic-helpers": "^1.0.3",
    "prismic-richtext": "^1.0.3",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "mocha": "^6.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^13.3.2"
  }
}
